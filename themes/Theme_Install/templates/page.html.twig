<header>
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="navigation">
                    <!-- Fixed navbar -->
                    <nav class="navbar navbar-default navbar-fixed-top">
                      <div class="container">
                        <div class="header-top">
                            <div class="col-sm-4">
                                <div class="call-us">
                                    <img src="themes/yg_restaurant_lite/assets/images/phone.png" class="img-responsive">
                                    <span>{{ phone }}</span>
                                </div>
                            </div>
                            <div class="col-sm-4">
                                <div class="logo">
                                    {% if logopath  %}
                                    <a href="{{ path('<front>') }}"><img src="{{ logopath }}" class="img-responsive"></a>
                                    {% endif %}
                                </div>
                            </div>
                            <div class="col-sm-4">
                                <div class="social-icon">
                                    <ul>
                                        {% if facebook  %}
                                        <li><a href="{{ facebook }}"><i class="fa fa-facebook"></i></a></li>
                                        {% endif %}

                                        {% if twitter  %}
                                        <li><a href="{{ twitter }}"><i class="fa fa-twitter"></i></a></li>
                                        {% endif %}

                                        {% if google_plus  %}
                                        <li><a href="{{ google_plus }}"><i class="fa fa-google-plus"></i></a></li>
                                        {% endif %}

                                        {% if linkedin  %}
                                        <li><a href="{{ linkedin }}"><i class="fa fa-linkedin"></i></a></li>
                                        {% endif %}
                                    </ul>
                                </div>
                            </div>
                        </div>
                        <div class="header-bottom">
                            <div class="navbar-header">
                              <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                              </button>
                            </div>
                            <div id="navbar" class="navbar-collapse collapse">
                              {{ page.primary_menu }}   
                            </div><!--/.nav-collapse -->
                        </div>
                      </div>
                    </nav>
                </div>
            </div>
        </div>
    </div>
</header>
 <!--header section end-->
{% set url = url('<current>') %}
{% if 'user' in url|render|render or 'node' in url|render|render %}
<section class="menu-banner text-center">
    <div class="inner-overlay"></div>
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="header-content">
                    <h1>{{ page.page_title }}</h1>
                </div> 
            </div>
        </div>
    </div>
</section>
{% endif %}


 <a id="main-content"></a>
          {{ page.content }}

    
<!--footer section start-->
<footer class="footer-sec">
    <div class="container">
        <div class="row">
            <div class="col-md-4">
                <div class="contact-info contact-inner">
                    {{ page.footer_col_1 }}
                </div>
            </div>
            <div class="col-md-4">
                <div class="opening-hours">
                    {{ page.footer_col_2 }}
                </div>
            </div>
            <div class="col-md-4">
                <div class="subscribe">
                   {{ page.footer_col_3 }}
                </div>
            </div>
            <div class="col-md-12">
                <div class="copyright text-center">
                    <p>Copyright 2018. All Rights Reserved. Developed by <span class="yellow">NavTheme.</span>Theme by<a href="https://www.drupaldevelopersstudio.com" target="_blank"> Drupal Developers Studio.</a></p>
                </div>
            </div>
        </div>
    </div>
</footer>