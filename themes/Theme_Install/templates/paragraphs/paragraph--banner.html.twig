
<!--header banner section start-->
<section class="header-banner text-center" style="background-image:url('{{ file_url(content.field_banner_image['#items'].entity.uri.value) }}');">
    <div class="overlay" style="background-image:url({{ file_url(content.field_banner_overlay_image['#items'].entity.uri.value) }});"></div>
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="header-content">
                    <h1>{{ content.field_banner_title.0 }}</h1>
                    <p>{{ content.field_banner_sub_titl.0 }}</p>
                    <!-- <button class="btn btn-sm btn-default">Order Now</button> -->
                    <a class="btn btn-sm btn-default" href="{{ content.field_banner_btn[0]['#url']|render }}">{{ content.field_banner_btn[0]['#title']|render }}</a>
                </div> 
            </div>
        </div>
    </div>
</section>
<!--header banner section end-->