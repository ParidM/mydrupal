<!--our menu section start-->
<section id="menu" class="menu-sec" style="background-image: url('{{ file_url(content.field_free_delivery_bg_image['#items'].entity.uri.value) }}');">
    <div class="menu-overlay"></div>
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="main-heading">
                    <span>{{ content.field_about_sub_title.0}}</span>
                    <h2>{{ content.field_about_title.0}}</h2>
                    <img src="{{ file_url(content.field_about_header_image['#items'].entity.uri.value) }}" class="img-responsive">
                </div>
            </div>

            <div class="col-sm-6">
                <ul class="menu-wrapper">
                  {{ content.field_menu_left}}
                </ul>
            </div>


            <div class="col-sm-6">
                <ul class="menu-wrapper">
                    {{ content.field_menu_right}}
                </ul>
            </div>
            
            <div class="col-md-12 text-center">
                <a href="{{ content.field_about_btn[0]['#url']|render }}" class="btn btn-sm btn-default">{{ content.field_about_btn[0]['#title']|render }}</a>
            </div>
        </div>
    </div>
</section>
<!--our menu section end-->
    